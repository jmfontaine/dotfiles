abbr ".."="cd .."
abbr "..."="cd ../.."
abbr "...."="cd ../../.."
abbr "....."="cd ../../../.."
abbr "......"="cd ../../../../.."
abbr "......."="cd ../../../../../.."
abbr "b"="brew"
abbr "c"="code ."
abbr "g"="git"
abbr "git ci"="git commit"
abbr "git co"="git checkout"
abbr "git oops"="git reset HEAD^ --soft"
abbr "git st"="git status"
abbr "h"="history"
abbr "ll"="eza -agl --color=always --group-directories-first --icons=auto --time-style=long-iso"
abbr "uve"="UV_ENV_FILE=.env uv"
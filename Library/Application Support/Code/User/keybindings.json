// cSpell:disable
[
  {
    "key": "cmd+k cmd+k",
    "command": "toggle",
    "when": "editorTextFocus",
    "args": {
      "id": "settings",
      "value": [
        {
          "editor.fontSize": 14,
          "terminal.integrated.fontSize": 14,
          "workbench.panel.defaultLocation": "bottom"
        },
        {
          "editor.fontSize": 15,
          "terminal.integrated.fontSize": 15,
          "workbench.panel.defaultLocation": "right"
        }
      ]
    }
  }
]

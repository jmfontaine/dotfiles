abbr ".."="cd .."
abbr "..."="cd ../.."
abbr "...."="cd ../../.."
abbr "....."="cd ../../../.."
abbr "......"="cd ../../../../.."
abbr "......."="cd ../../../../../.."
abbr "c"="code"
abbr "g"="git"
abbr "git ci"="git commit"
abbr "git co"="git checkout"
abbr "git oops"="git reset HEAD^ --soft"
abbr "git st"="git status"
abbr "h"="history"
abbr "ll"="eza -al --icons"
abbr "tf"="terraform"
abbr "yadm ci"="yadm commit"
abbr "yadm st"="yadm status"

"*":
  core:
    disabledPackages: [
      "exception-reporting"
      "markdown-preview"
      "metrics"
    ]
    projectHome: "~/Projects"
    telemetryConsent: "no"
    themes: [
      "seti-ui"
      "monokai-seti"
    ]
  editor:
    fontFamily: "Inconsolata-dz for Powerline"
    fontSize: 12
    invisibles: {}
    lineHeight: 1.2
    showInvisibles: true
    tabLength: 4
    tabType: "auto"
  "highlight-line": {}
  linter: {}
  "linter-ui-default":
    showPanel: true
  "markdown-preview-plus":
    useGitHubStyle: true
  "seti-ui":
    compactView: true
    themeColor: "Green"
  welcome:
    showOnStartup: false

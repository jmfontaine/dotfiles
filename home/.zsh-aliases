alias ..="cd .."
alias ...="cd ../../"
alias ....="cd ../../../"
alias .....="cd ../../../../"
alias ......="cd ../../../../../"

alias d="cd ~/Documents/Dropbox"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias g="git"
alias gh="history | grep"
alias h="history"
alias ip="dig +short myip.opendns.com @resolver1.opendns.com"
alias ls="ls -Fhp"
alias ll="ls -aFhlp"
alias reload="source ~/.zshrc"
alias sshkey="cat ~/.ssh/id_rsa.pub | pbcopy && echo 'Copied to clipboard.'"
alias tree="tree -Csu"

# Enable aliases to be sudo’ed
alias sudo='sudo '

# Get OS X Software Updates, and update installed Ruby gems, Homebrew, npm, and their installed packages
alias update='sudo softwareupdate -i -a; brew update; brew upgrade --all; brew cleanup; npm update npm -g; npm update -g; sudo gem update --system; sudo gem update --no-document'

# Vagrant
alias v="vagrant"
alias vdes="vagrant destroy"
alias vhalt="vagrant halt"
alias vprov="vagrant provision"
alias vrel="vagrant reload"
alias vssh="vagrant ssh"
alias vsus="vagrant suspend"
alias vup="vagrant up"

# Flush Directory Service cache
alias flush="dscacheutil -flushcache && killall -HUP mDNSResponder"

# Clean up LaunchServices to remove duplicates in the “Open With” menu
alias lscleanup="/System/Library/Frameworks/CoreServices.framework/Frameworks/LaunchServices.framework/Support/lsregister -kill -r -domain local -domain system -domain user && killall Finder"

# s3cmd
alias s3cmd-empideal="s3cmd -c ~/.s3cfg-empideal"
alias s3cmd-jmf="s3cmd -c ~/.s3cfg-jmf"
alias s3cmd-dev="s3cmd -c ~/.s3cfg-dev"
alias s3cmd-prod="s3cmd -c ~/.s3cfg-prod"

# Elastic Beanstalk
alias eb-jmf="eb --profile=jmf"
alias eb-dev="eb --profile=dev"
alias eb-prod="eb --profile=prod"